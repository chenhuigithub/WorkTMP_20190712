<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:clickable="false"
    android:gravity="center"
    android:orientation="vertical"
    android:padding="@dimen/dp_008"
    tools:context=".aty.MainActivity">


    <ImageView
        android:id="@+id/iv_logo_layout_aty_choose_question_bank"
        android:layout_width="@dimen/dp_020"
        android:layout_height="@dimen/dp_020"
        android:background="@drawable/ic_launcher"
        android:scaleType="fitXY" />

    <TextView
        android:id="@+id/tv_name_layout_aty_choose_question_bank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_006"
        android:text="题库"
        android:textColor="@color/color_text_title"
        android:textSize="@dimen/sp_008" />


</LinearLayout>